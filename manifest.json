{
  "name": "Nyaman Tanpa Iklan",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Nyaman Tanpa Iklan",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*.kompas.com/*",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.kompas.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}